{
  "instance_id": "nervedev_machine",
  "service_checks": {
    "stupid_tcp": {
      "port": 4321,
      "check_interval": 3,
      "checks": [
        {
          "type": "tcp",
          "timeout": 0.2,
          "rise": 2,
          "fall": 1
        }
      ],
      "zk_connstring": "127.0.0.1:2181,127.0.0.1:3181,127.0.0.1:4181",
      "zk_root": "/nervedev/services/stupid_tcp/services",
      "host": "127.0.0.1"
    },
    "stupid_http": {
      "port": 8080,
      "check_interval": 3,
      "checks": [
        {
          "type": "http",
          "uri": "/",
          "timeout": 2,
          "rise": 2,
          "fall": 1
        }
      ],
      "zk_connstring": "127.0.0.1:2181,127.0.0.1:3181,127.0.0.1:4181",
      "zk_root": "/nervedev/services/stupid_http/services",
      "host": "127.0.0.1"
    }
  }
}
